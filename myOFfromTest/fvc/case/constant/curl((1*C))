FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "constant";
    object      curl((1*C));
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 -0 0);

boundaryField
{
    my_cube_faces
    {
        type            calculated;
        value           uniform (0 -0 0);
    }
}


